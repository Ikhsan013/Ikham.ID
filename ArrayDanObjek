<!DOCTYPE html>
<html>
<head>
    <title>Array dengan Objek</title>
</head>
<body>

<script>   
    var siswa = [
        { id: 01, nama: "ikhsan", umur: 17, kelas: "XI RPL 1" },
        { id: 02, nama: "gina", umur: 19, kelas: "XI RPL 1" },
        { id: 03, nama: "restu", umur: 15, kelas: "XI RPL 1" }
    ];

    for (var i = 0; i < siswa.length; i++) {
        console.log("ID: " + siswa[i].id);
        console.log("Nama: " + siswa[i].nama);
        console.log("Umur: " + siswa[i].umur);
        console.log("Kelas: " + siswa[i].kelas);
        console.log("\n");
    }
</script>

</body>
</html>
